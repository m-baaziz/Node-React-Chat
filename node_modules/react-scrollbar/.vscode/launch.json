{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch index.html",
			"type": "chrome",
			"request": "launch",
			"file": "index.html"
		},
		{
			"name": "Launch localhost with sourcemaps",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8003",
			"sourceMaps": true,
			"outDir": "out"
		},
		{
			"name": "Attach",
			"type": "chrome",
			"request": "attach",
			"port": 8003
		},
		{
			"name": "Debug Mocha Test",
			"type": "node",
			"address": "localhost",
			"port": 9876,
			"sourceMaps": false
		}
	]
}